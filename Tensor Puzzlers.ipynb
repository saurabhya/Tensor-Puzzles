    "import torch\n",
    "\n",
    "\n",
    "def arange(i: int):\n",
    "    \"Think for-loop\"\n",
    "    return torch.tensor(range(i))\n",
    "\n",
    "\n",
    "def where(q, a, b):\n",
    "    \"Think if-statement\"\n",
    "    return (q * a) + (~q) * b"
